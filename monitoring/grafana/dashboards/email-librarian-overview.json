{
  "dashboard": {
    "id": null,
    "title": "Email Librarian System Overview",
    "tags": ["email-librarian"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Email Processing Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(email_processing_total[5m])",
            "legendFormat": "Emails/sec"
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 }
      },
      {
        "id": 2,
        "title": "Agent Execution Count",
        "type": "graph",
        "targets": [
          {
            "expr": "agent_execution_total",
            "legendFormat": "{{agent_name}}"
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 }
      },
      {
        "id": 3,
        "title": "Vector Search Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(vector_search_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 }
      },
      {
        "id": 4,
        "title": "System Resource Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "process_resident_memory_bytes",
            "legendFormat": "Memory Usage"
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "5s"
  }
}
